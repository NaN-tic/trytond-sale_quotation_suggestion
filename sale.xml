<tryton>
    <data>
        <record model="ir.ui.view" id="carrier_sale_suggestion_view_form">
            <field name="model">sale.quotation.suggestion</field>
            <field name="type">form</field>
            <field name="name">sale_quotation_suggestion_form</field>
